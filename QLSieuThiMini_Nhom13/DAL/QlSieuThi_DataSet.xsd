                    <CommandText>SELECT    sp.MaSP, sp.TenSP, MAX(hd.NgayLap) AS NgayLap, SUM(cthd.SoLuong) AS SoLuongBan, SUM(cthd.SoLuong * cthd.DonGia) AS DoanhThu
